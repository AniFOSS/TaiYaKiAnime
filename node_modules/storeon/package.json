{
  "name": "storeon",
  "version": "3.0.3",
  "description": "Tiny (167 bytes) event-based Redux-like state manager for React and Preact",
  "keywords": [
    "state",
    "immutable",
    "react",
    "preact",
    "hook"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "storeon/storeon",
  "sideEffects": false,
  "peerDependencies": {
    "preact": "^10.0.0",
    "react": "^16.8.0"
  },
  "peerDependenciesMeta": {
    "preact": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "react-native": "index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./index.cjs",
      "import": "./index.js"
    },
    "./devtools/package.json": "./devtools/package.json",
    "./devtools": {
      "require": "./devtools/index.cjs",
      "import": "./devtools/index.js"
    },
    "./preact/package.json": "./preact/package.json",
    "./preact": {
      "require": "./preact/index.cjs",
      "import": "./preact/index.js"
    },
    "./react/package.json": "./react/package.json",
    "./react": {
      "require": "./react/index.cjs",
      "import": "./react/index.js"
    }
  }
}